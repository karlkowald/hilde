// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

model Match {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  team1     String
  team2     String
  score1    Int
  score2    Int
  comment   String
  game      String

  @@map("match")
}
